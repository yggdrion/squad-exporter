{
    "extends": [
        "config:recommended",
        ":dependencyDashboard",
        "helpers:pinGitHubActionDigests"
    ],
    "schedule": [],
    "assignees": [
        "r4b2"
    ],
    "labels": [
        "dependencies"
    ],
    "semanticCommits": "enabled",
    "packageRules": [
        {
            "matchManagers": [
                "github-actions"
            ],
            "groupName": "github-actions",
            "commitMessagePrefix": "chore(github-actions): ",
            "pinDigests": true,
            "digestOneAndOnly": true
        },
        {
            "matchManagers": [
                "gomod"
            ],
            "groupName": "go",
            "commitMessagePrefix": "chore(go): "
        }
    ]
}
